{
  "$schema": "https://json-schema.org/draft-07/schema",
  "$id": "http://example.com/example.json",
  "type": "object",
  "default": {},
  "title": "Root Schema",
  "required": [
      "title",
      "company_handle"
  ],
  "properties": {
      "title": {
          "type": "string",
          "minLength": 1,
          "maxLength": 50
      },
      "salary": {
          "type": "integer",
          "minimum": 0
      },
      "equity": {
          "type": "integer",
          "pattern": "0|(0?\\.[0-9]+)"
      },
      "company_handle": {
          "type": "string",
          "minLength": 1,
          "maxLength": 30
      }
  }
}